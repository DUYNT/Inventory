<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://jakarta.apache.org/struts/dtds/struts-config_1_3.dtd">
 
<struts-config>
	<action-mappings>
        <action path="/Station_init" name="BaseForm" scope="request" validate="false">
            <forward name="success" path="STATION" />
            <forward name="failure" path="STATION" />
        </action>
        <action path="/Station_adding" name="BaseForm" scope="request" validate="false">
            <forward name="success" path="STATION" />
            <forward name="failure" path="STATION" />
        </action>
        <action path="/Station_editing" name="BaseForm" scope="request" validate="false">
            <forward name="success" path="STATION" />
            <forward name="failure" path="STATION" />
        </action>
	</action-mappings>
</struts-config>