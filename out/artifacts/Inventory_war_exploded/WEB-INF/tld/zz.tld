        <tag-class>com.tecapro.inventory.common.tag.MSValueBase64Tag</tag-class>
        <body-content>empty</body-content>
        <description>
            <![CDATA[
            Tag to store as base64 data the value class of business to hidden
        ]]>
        </description>
    </tag>
    
    <tag>
        <name>msg</name>
        <tag-class>com.tecapro.inventory.common.tag.MSMsgTag</tag-class>
        <body-content>empty</body-content>
        <description>
            <![CDATA[
            Write message in file [message.properties.xml]
        ]]>
        </description>
        <attribute>
            <name>msgCode</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
                <![CDATA[
            Message code in file [message.properties.xml]
            ]]>
            </description>
        </attribute>
        <attribute>
            <name>params</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
                <![CDATA[
                Parameters put into message.
            Format parameter: param1||param2||param3||.....
            ]]>
            </description>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description/>
        </attribute>
        <attribute>
            <name>withCode</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
                <![CDATA[
                Default is true
            ]]>
            </description>
        </attribute>
        <attribute>
            <name>filter</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description>
                <![CDATA[
                Default is true
            ]]>
            </description>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <description/>
        </attribute>
    </tag>
</taglib>